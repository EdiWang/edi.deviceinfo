<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Company>edi.wang</Company>
        <Authors>Edi Wang</Authors>
        <Description>.NET type for edi.deviceinfo web package</Description>
        <PackageTags>Device</PackageTags>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://edi.wang</PackageProjectUrl>
        <RepositoryType></RepositoryType>
        <RepositoryUrl>https://github.com/EdiWang/edi.deviceinfo</RepositoryUrl>
        <TargetFrameworks>net6.0;net8.0</TargetFrameworks>
    </PropertyGroup>

</Project>
